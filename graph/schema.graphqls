# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Competition {
  year: String!
  country: String!

  prevCompetition: Competition
  nextCompetition: Competition
}

type Player {
  name: String!
}

type Query {
  allCompetition: [Competition]!
  allPlayer: [Player]!
}

